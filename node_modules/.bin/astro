#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/spenserwellen/Documents/GitHub/Vado.com/node_modules/.pnpm/astro@4.16.7_@types+node@22.8.4_lightningcss@1.27.0_rollup@4.24.3_terser@5.34.1_typescript@5.6.3/node_modules/astro/node_modules:/Users/spenserwellen/Documents/GitHub/Vado.com/node_modules/.pnpm/astro@4.16.7_@types+node@22.8.4_lightningcss@1.27.0_rollup@4.24.3_terser@5.34.1_typescript@5.6.3/node_modules:/Users/spenserwellen/Documents/GitHub/Vado.com/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/spenserwellen/Documents/GitHub/Vado.com/node_modules/.pnpm/astro@4.16.7_@types+node@22.8.4_lightningcss@1.27.0_rollup@4.24.3_terser@5.34.1_typescript@5.6.3/node_modules/astro/node_modules:/Users/spenserwellen/Documents/GitHub/Vado.com/node_modules/.pnpm/astro@4.16.7_@types+node@22.8.4_lightningcss@1.27.0_rollup@4.24.3_terser@5.34.1_typescript@5.6.3/node_modules:/Users/spenserwellen/Documents/GitHub/Vado.com/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
